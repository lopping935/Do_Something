<?xml version="1.0" encoding="utf-8"?>
<ParamList>	
	<!-- 地址相关 -->
	<ParamItem>
		<Name>Slot</Name>
		<DefaultValue>0</DefaultValue>
		<Description>槽号，整型</Description>
	</ParamItem>
	<ParamItem>
		<Name>IntDataType</Name>
		<DefaultValue>1</DefaultValue>
		<Description>数据类型，整型，从1开始顺序传递，INT16 UINT16 INT32 UINT32</Description>
	</ParamItem>
	<ParamItem>
		<Name>TagName</Name>
		<DefaultValue>TAG1</DefaultValue>
		<Description>标签名称，字符串，输入</Description>
	</ParamItem>
	<ParamItem>
		<Name>PollEnable</Name>
		<DefaultValue>false</DefaultValue>
		<Description>轮询使能，true：打开轮询使能，flase：关闭轮询使能</Description>
	</ParamItem>
	<ParamItem>
		<Name>OverTime</Name>
		<DefaultValue>200</DefaultValue>
		<Description>超时时间，整型 ms</Description>
	</ParamItem>
	<ParamItem>
		<Name>Upload</Name>
		<DefaultValue>true</DefaultValue>
		<Description>true：打开数据上传，flase：关闭数据上传</Description>
	</ParamItem>
	<!-- 地址相关 -->
</ParamList>