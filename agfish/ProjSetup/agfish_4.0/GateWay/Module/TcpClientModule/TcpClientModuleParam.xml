<?xml version="1.0" encoding="utf-8"?>
<ParamList>
	<ParamItem>
		<Name>ReConnectState</Name>
		<DefaultValue>flase</DefaultValue>
		<Description>true：打开重连使能，flase：关闭重连使能</Description>
	</ParamItem>
	<ParamItem>
		<Name>Upload</Name>
		<DefaultValue>true</DefaultValue>
		<Description>true：打开数据上传，flase：关闭数据上传</Description>
	</ParamItem>
	<ParamItem>
		<Name>HostIP</Name>
		<DefaultValue></DefaultValue>
		<Description>主机ip</Description>
	</ParamItem>
	<ParamItem>
		<Name>HostPort</Name>
		<DefaultValue>0</DefaultValue>
		<Description>主机port</Description>
	</ParamItem>
	<ParamItem>
		<Name>TargetIP</Name>
		<DefaultValue>127.0.0.1</DefaultValue>
		<Description>目标ip</Description>
	</ParamItem>
	<ParamItem>
		<Name>TargetPort</Name>
		<DefaultValue>7920</DefaultValue>
		<Description>目标port</Description>
	</ParamItem>
	<ParamItem>
		<Name>EndCodeEnable</Name>
		<DefaultValue>false</DefaultValue>
		<Description>true：打开结束符使能，flase：关闭结束符使能</Description>
	</ParamItem>
	<ParamItem>
		<Name>EndCode</Name>
		<DefaultValue></DefaultValue>
		<Description>字符串，结束符内容</Description>
	</ParamItem>
	<ParamItem>
		<Name>ConnectState</Name>
		<DefaultValue>flase</DefaultValue>
		<Description>true：打开连接，flase：关闭连接</Description>
	</ParamItem>
</ParamList>