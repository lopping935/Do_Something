<?xml version="1.0" encoding="utf-8"?>
<ParamList>
	<ParamItem>
		<Name>ReConnectState</Name>
		<DefaultValue>false</DefaultValue>
		<Description>true：打开重连使能，flase：关闭重连使能</Description>
	</ParamItem>
	<ParamItem>
		<Name>Upload</Name>
		<DefaultValue>true</DefaultValue>
		<Description>true：打开数据上传，flase：关闭数据上传</Description>
	</ParamItem>
	<ParamItem>
		<Name>ComPort</Name>
		<DefaultValue>1</DefaultValue>
		<Description>串口号：combox,0~15</Description>
	</ParamItem>
	<ParamItem>
		<Name>Baudrate</Name>
		<DefaultValue>9600</DefaultValue>
		<Description>波特率:combox,110,300,600,1200,2400,4800,9600,14400,19200,38400,56000,57600,115200,128000,256000</Description>
	</ParamItem>
	<ParamItem>
		<Name>DataBits</Name>
		<DefaultValue>8</DefaultValue>
		<Description>数据位:combox,5,6,7,8</Description>
	</ParamItem>
	<ParamItem>
		<Name>Parity</Name>
		<DefaultValue>0</DefaultValue>
		<Description>校验位:combox,0~4</Description>
	</ParamItem>
	<ParamItem>
		<Name>StopBits</Name>
		<DefaultValue>0</DefaultValue>
		<Description>停止位:combox,0,1,2</Description>
	</ParamItem>
	<ParamItem>
		<Name>ReadIntervalTimeout</Name>
		<DefaultValue>5</DefaultValue>
		<Description>字符之间最大的延时:edit</Description>
	</ParamItem>
	<ParamItem>
		<Name>ReadTime</Name>
		<DefaultValue>100</DefaultValue>
		<Description>读超时:edit</Description>
	</ParamItem>
	<ParamItem>
		<Name>ConnectState</Name>
		<DefaultValue>false</DefaultValue>
		<Description>true：打开连接，flase：关闭连接</Description>
	</ParamItem>
</ParamList>